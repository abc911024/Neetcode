#include <iostream>
#include <vector>
class Solution {
public:
    std::vector<int> getConcatenation(std::vector<int>& nums) {
        int n = nums.size();
        std::vector<int> arr(n*2);
        for (int i=0;i<n;i++){
            arr[i] = nums[i];
            arr[i+n] = nums[i];
        }
        return arr;
    }
};